{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) split_compress",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/split_compress_ms.x",
        "args": [
            "-ms_path", "human_chimp_from_fabio.ms",
            "-len", "100", "-cnt1", "100000",
            "-no_check", "1"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/code_playground/split_compress",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "build split_compress"
    },
    ]
}