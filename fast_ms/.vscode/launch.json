{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) query_profile",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/range_queries_profile.x",
        "args": [
            "-ms_path", "../experiments/range_tests/vanilla_compression_techniques/mm.t.ms.none",
            "-from_max_idx", "800000", "-range_size", "400000",
            "-compression", "none",
            "-ridx_path ../experiments/range_tests/vanilla_compression_techniques/mm.t.ms.32.ridx",
            "-block_size" "32",
            "-niter", "1"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "build range_queries_profile"
    },
    ]
}