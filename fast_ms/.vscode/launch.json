{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) range queries",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/range_queries.x",
        "args": [
            "-ms_path", "./tests/input/rnd_200_64.t.ms",
            "-compression", "none",
            "-block_size", "4",
            "-ridx_path", "./tests/range_queries_djamal_method/rnd_200_64.t.ms.4.ridx",
            "-from_idx", "3", "-to_idx", "4",
            "-check", "1"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "build range_queries"
    },
    {
        "name": "(gdb) range index dump",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/dump_range_index.x",
        "args": [
            "-ms_path", "./code_playground/range_queries/mut_200s_64t_15.t.ms",
            "-block_size", "5",
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "build dump_range_index"
    },
    ]
}